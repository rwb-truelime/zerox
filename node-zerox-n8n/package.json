{
  "name": "n8n-nodes-zerox",
  "version": "0.1.0",
  "type": "module",
  "description": "N8N node for Zerox OCR & Document Extraction",
  "keywords": [
    "n8n-community-node-package",
    "zerox",
    "ocr",
    "document-extraction",
    "vision-models"
  ],
  "license": "MIT",
  "homepage": "https://github.com/getomni-ai/zerox",
  "author": {
    "name": "Omni AI",
    "email": "info@getomni.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/getomni-ai/zerox.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ZeroxApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Zerox/Zerox.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "eslint-plugin-prettier": "^5.2.6",
    "n8n-workflow": "*",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "dependencies": {
    "pnpm": "^10.8.0",
    "zerox-truelime": "file:../zerox-truelime-1.1.15.tgz"
  }
}
